    'Change' AS MessageName, 
    'Data Flow Task' AS TaskName, 
    'StagingTimesheet' AS TableName, 
    @@ROWCOUNT AS RowsLoaded, 
    GETDATE() AS RunDate, 
    SUSER_SNAME() AS ExecutedBy, 
    NULL AS EmployeeName, 
    NULL AS SheetName

UNION ALL

SELECT 
    'Success' AS MessageName, 
    'Data Flow Task' AS TaskName, 
    'StagingTimesheet' AS TableName, 
    0 AS RowsLoaded,  -- You can set this to 0 or a custom flag
    GETDATE(), 
    SUSER_SNAME(), 
    NULL, 
    NULL;</property>
